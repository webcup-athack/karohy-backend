{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [{ "src": "/.*", "dest": "build/server.js" }],
  "env": {
    "BUILD_SCRIPT": "npm run build"
  },
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": false,
      "staging": false
    }
  }
}

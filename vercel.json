{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "package.json"
    }
  ],
  "env": {
    "BUILD_SCRIPT": "npm run build",
    "NODE_ENV": "production"
  },
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": false,
      "staging": false
    }
  }
}
